<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:main_title="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0eff5"
    android:orientation="vertical">

    <com.idxk.mobileoa.android.ui.views.widget.HomeCommonTitle
        android:id="@+id/appAlarm_mainTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        main_title:homeLeftImage="@drawable/title_back"
        main_title:homeTitle="@string/fragment_home_fileList"></com.idxk.mobileoa.android.ui.views.widget.HomeCommonTitle>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:layout_marginTop="35dp"
        android:background="#c7c7c9"></LinearLayout>

    <ListView
        android:id="@+id/items"
        style="@style/comsoft_Listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="#c7c7c9"
        android:dividerHeight="1px"></ListView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#c7c7c9"></LinearLayout>


</LinearLayout>