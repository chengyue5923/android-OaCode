<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="MainTitleView">
        <attr name="showLeft" format="boolean"/>
        <attr name="showRight" format="boolean"/>
        <attr name="showTitle" format="boolean"/>
        <attr name="showSelf" format="boolean"/>
        <attr name="showRightText" format="boolean"/>
        <attr name="titleCenter" format="string"/>
        <attr name="leftImage" format="reference"/>
        <attr name="rightImagePress" format="reference"/>
        <attr name="leftImagePress" format="reference"/>
        <attr name="rightImage" format="reference"/>
        <attr name="rightText" format="string"/>
        <attr name="showTrigon" format="boolean"/>
